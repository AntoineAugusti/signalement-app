{
  "name": "signalement-app",
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start:local": "npm run generate-json-files && npm run generate-blog-metadata && ng serve --host 0.0.0.0",
    "start": "node dist/server.js",
    "prebuild": "npm run generate-json-files && npm run generate-blog-metadata && envsub --env API_BASE_URL ./src/environments/environment.prod.ts",
    "build": "ng build",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run prebuild && npm run build:prod",
    "build:server:prod": "ng run signalement-app:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --prod",
    "build:prod": "npm run build:server:prod && npm run build:browser:prod",
    "heroku-postbuild": "echo Skip build on Heroku",
    "generate-json-files": "node src/scripts/generate-json-files.ts",
    "generate-blog-metadata": "node src/scripts/generate-blog-metadata.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cli": "~8.3.18",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/http": "^7.2.3",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/platform-server": "^8.2.13",
    "@angular/router": "^8.2.13",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-toolkit/universal": "^1.1.50",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "@ngx-pwa/local-storage": "^6.2.2",
    "angulartics2": "7.2.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "echarts": "^4.2.1",
    "envsub": "^3.1.0",
    "express": "^4.16.4",
    "har-validator": "^5.1.3",
    "helmet": "^3.21.1",
    "jquery": "^3.3.1",
    "js-yaml": "^3.13.1",
    "json-typescript-mapper": "^1.1.3",
    "moment": "^2.22.2",
    "ng2-completer": "^3.0.0-beta.2",
    "ngx-bootstrap": "^4.3.0",
    "ngx-echarts": "^4.2.1",
    "ngx-loading": "^8.0.0",
    "ngx-markdown": "^8.0.2",
    "path": "^0.12.7",
    "rxjs": "~6.5.3",
    "ts-loader": "4.2.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular/language-service": "^8.2.13",
    "@types/echarts": "^4.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
